import WidgetGUI
import VisualAppBase

public struct Renderables<S: System<W, R>, W: Window, R: Renderer> {
    public typealias Root = WidgetGUI.Root<S, W, R>
    public typealias Row = WidgetGUI.Row<S, W, R>
    public typealias Column = WidgetGUI.Column<S, W, R>
    public typealias Background = WidgetGUI.Background<S, W, R>
    public typealias Divider = WidgetGUI.Divider<S, W, R>
    public typealias Text = WidgetGUI.Text<S, W, R>
    public typealias Button = WidgetGUI.Button<S, W, R>
    public typealias Space = WidgetGUI.Space<S, W, R>
    public typealias ComputedSize = WidgetGUI.ComputedSize<S, W, R>
}